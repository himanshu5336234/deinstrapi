{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "name": "Services",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "form": {
      "type": "component",
      "repeatable": true,
      "component": "form.service-form",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "gigs": {
      "via": "serivce",
      "collection": "gigs"
    }
  }
}
